class Book:
    def __init__(self, title, author):
        self.title = title
        self.author = author
book1 = Book("1949", "George Oneil")
print(book1.title,book1.author)
del book1.author
print(book1.author)
book2 = Book("1895", "Oliver Avellanoza")
print(book1.title,book1.author, book2.title,book2.author)
